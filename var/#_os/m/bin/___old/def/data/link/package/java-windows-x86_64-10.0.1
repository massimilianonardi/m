LINK="http://download.oracle.com/otn-pub/java/jdk/10.0.1+10/fb4372174a714e6b8c52526dc134031e/jdk-10.0.1_windows-x64_bin.exe"
HASH=""

download()
{
  DOWNLOAD_FILE="$1"
  if [ ! -s "$DOWNLOAD_FILE" ]
  then
    echo download "$LINK"
    wget -c --no-cookies --no-check-certificate --header "Cookie: oraclelicense=accept-securebackup-cookie" -O "$DOWNLOAD_FILE" "$LINK"
    
    if [ -s "$DOWNLOAD_FILE" ]
    then
      echo downloaded file $DOWNLOAD_FILE
    else
      echo download failed
      exit 1
    fi
  fi
}
