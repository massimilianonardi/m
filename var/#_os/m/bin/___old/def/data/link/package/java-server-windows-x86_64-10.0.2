LINK="http://download.oracle.com/otn-pub/java/jdk/10.0.2+13/19aef61b38124481863b1413dce1855f/serverjre-10.0.2_windows-x64_bin.tar.gz"
HASH=""

download()
{
  DOWNLOAD_FILE="$1"
  if [ ! -s "$DOWNLOAD_FILE" ]
  then
    echo download "$LINK"
    wget -c --no-cookies --no-check-certificate --header "Cookie: oraclelicense=accept-securebackup-cookie" -O "$DOWNLOAD_FILE" "$LINK"
    
    if [ -s "$DOWNLOAD_FILE" ]
    then
      echo downloaded file $DOWNLOAD_FILE
    else
      echo download failed
      exit 1
    fi
  fi
}
