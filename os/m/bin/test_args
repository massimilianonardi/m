  lang log_warn "called rec_5" @'stack' "msg_arg1" "msg_arg2" "msg_arg3"! "test" "test2"
  lang log_warn "called rec_5" @@'stack' "msg_arg1" "msg_arg2" "msg_arg3"! "test" "test2"
  log_info "called rec_5" "-$TRACE_STACK-"
#  echo "called rec_5" "-$TRACE_STACK-"
#env_list
}

